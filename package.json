{
  "name": "blablabook",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "lint": "eslint backend frontend",
    "lint:fix": "eslint backend frontend --fix",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up",
    "docker:build": "docker-compose -f docker-compose.dev.yml build --no-cache",
    "docker:clean": "docker-compose -f docker-compose.dev.yml down -v && docker system prune -f",
    "docker:install": "docker-compose -f docker-compose.dev.yml run --rm api npm install",
    "docker:shell": "docker-compose -f docker-compose.dev.yml run --rm api sh"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "eslint": "^9.33.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-unused-imports": "^4.2.0"
  }
}
